[swarm_master]
swarm_master ansible_host={SERVER NAME OR IP}
[swarm_master:vars]
ansible_user=jenkins
ansible_ssh_common_args='-o StrictHostChecking=no'
ansible_ssh_private_key_file=/home/jenkins/.ssh/id_rsa
[swarm_workers]
swarm_worker_1 ansible_host={SERVER NAME OR IP}
swarm_worker_2 ansible_host={SERVER NAME OR IP}
swarm_worker_3 ansible_host={SERVER NAME OR IP}
[swarm_workers:vars]
ansible_user=jenkins
anisble_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_ssh_private_key=/home/jenkins/.ssh/id_rsa
[nginx]
nginx ansible_host={SERVER NAME OR IP}
[nginx:vars]
ansible_user=jenkins
anisble_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_ssh_private_key=/home/jenkins/.ssh/id_rsa

