[managers]
manager ansible_host=manager
[managers:vars]
ansible_user=jenkins
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_ssh_private_key_file=/home/jenkins/.ssh/id_rsa
[workers]
worker ansible_host=worker
[workers:vars]
ansible_user=jenkins
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_ssh_private_key_file=/home/jenkins/.ssh/id_rsa
